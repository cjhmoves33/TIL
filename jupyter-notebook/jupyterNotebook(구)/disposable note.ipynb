    "def gcd(a, b): \n",
    "    while a != 0 and b != 0:\n",
    "        if a > b:\n",
    "            a = a - b\n",
    "        else:\n",
    "            b = b - a\n",
    "    return a+b\n",
    "\n",
    "gcd(1071, 1029)"
   ]
